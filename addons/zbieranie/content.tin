#alias {zbieranie} {
	#var tmploot {$ConfigManager_Configs[zbieranie]};
    #replace tmploot {,} {;};
    #list lootlist create {$tmploot};
};

#alias {_zbierz_}
{
  #loop {1} {$ilecial} {liz} {
	  	#foreach {*lootlist[]} {lol} {
	  		wez $lootlist[$lol] z $liz\. ciala;
	  	};
	};
};

#action {Bierzesz %*{klucz|kluczyk|pergamin|fetysz|karteczke|gwiazde|pieczec}%* z ciala %*} {
	#regex {%0} {%* bierze %* z ciala} {
		#var cowzial {&2};
	};
	#format kluczstamp {%t} {%Y-%h-%d %H:%M};
	#format kluczepoc {%T};
	
	#foreach {*klucze[]} {klu} {
		#if {$kluczepoc - $klucze[$klu][epoc] < 60 && $voom == $klucze[$klu][gdzie]} {
	   		#class klucze read bundles/ConfigManager/Profiles/klucze.tin;
			#class klucze open;
			#var klucze[$klu][klucz] {$cowzial};
	   		#class klucze close;
			#class klucze write bundles/ConfigManager/Profiles/klucze.tin;
			#echo {%-18s %-5s %-30s %-20s} {<148>$klu} {<128>$klucze[$klu][gdzie]} {<fac>$klucze[$klu][kogo] $klucze[$klu][imie]} {<138>$klucze[$klu][klucz]};
		};
	};
};


#action {%* bierze %*{klucz|kluczyk|pergamin|fetysz|karteczke|gwiazde|pieczec}%* z ciala %*} {
	#regex {%0} {%* bierze %* z ciala} {
