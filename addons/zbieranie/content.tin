};

#alias {/klucze} {
	#class klucze read bundles/ConfigManager/Profiles/klucze.tin;
	#class klucze close;

	#line ignore #showme {<dfd>Historia klucznikow:<088>};
	#foreach {*klucze[]} {klu} {
		#echo {%-18s %-5s %-30s %-20s} {<148>$klu} {<128>$klucze[$klu][gdzie]} {<fac>$klucze[$klu][kogo] $klucze[$klu][imie]} {<138>$klucze[$klu][klucz]};
	};
} {1};

#action {%* {umarl|umarla}.} {
	#var cowzial {};
	#local umarly {%1};
	#regex {$umarly} { } {
	    #replace umarly { } {_};
	    #format {umarly} {%l} {$umarly};
	};
    #local umarly {${umarly}_};
	#local imie {$postac[$umarly][imie]};

   	#if {"$postac[$umarly][gildia]" == "BOSS"} {   		
   		#format kluczstamp {%t} {%Y-%h-%d %H:%M};
  	 	#format kluczepoc {%T};

   		#class klucze read bundles/ConfigManager/Profiles/klucze.tin;
   		#class klucze open;
	   		#var klucze[$kluczstamp][epoc] {$kluczepoc};
	   		#var klucze[$kluczstamp][gdzie] {$voom};
	   		#var klucze[$kluczstamp][kogo] {%1};
	   		#var klucze[$kluczstamp][imie] {$imie};
	   		#var klucze[$kluczstamp][klucz] {<118>brak<088>};
   		#class klucze close;
		#class klucze write bundles/ConfigManager/Profiles/klucze.tin;

   		#line ignore #showme {<158>%1 UMARL ---- MOZE MIEC KLUCZ.};
   		#math {ilecial} {$ilecial + 1};
		qb _zbierz_;
		#line gag;
    } {
		#math {ilecial} {$ilecial + 1};
		qb _zbierz_;
    };
};

#action {Sila utrzymujaca %* opuszcza} {
  #math {ilecial} {$ilecial + 1};
  qb _zbierz_;	
}

#alias {/ql} {
	wez %0 z pierwszego ciala;
	wez %0 z drugiego ciala;
	wez %0 z trzeciego ciala;
	wez %0 z czwartego ciala;
	wez %0 z piatego ciala;
	wez %0 z szostego ciala;
} {1};
