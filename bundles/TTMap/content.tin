#alias findme {
        #map get roomvnum rmvnum;
        #var widocznewyjscia {$ROOM_INFO[exits]};
        #var exitList {};
        #var vexits {};

        #var roomfound 0;

    #nop ##################################;
    #nop szukaj pokoju wedlug jego opisu
    #nop w lokacjach gmcp, opis jest koordynata
    #nop w reszcie lokacji, ostatnio widziany short w danej lokacji
