                    #var pendingstep 0;
                    #map flag static on;
                };
            };
        };
    };
};

#nop #################################################################################;
#nop Komenda UNDO - uzywana do cofania kulki;
#nop #################################################################################;
#alias undo {
    #if {$trytomove == 1} {
        #map undo;
        #map goto $roomId;
        #var trytomove 0;
        #math pendingstep {$pendingstep - 1};
    };
    #if {$semifrozen_status == 1} {
        semifrozen_unset;   #nop wracam do zwyklego trybu;
        #undelay {PATH %*}; #nop zatrzymuje reszte komendy;
    };
}

