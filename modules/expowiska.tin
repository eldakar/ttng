#alias _expo_ {
	#echo {<118>Nie wybrano zadnego expowiska!<088>};
};

#function _szukajexpa_ {
	#foreach {*duch[]} {czytroll} {
		#regex {$duch[$czytroll][desc]} {troll} {
			#echo {<fca> * * * Wykrylem goblinoidy *** <088>};
			expowisko goblinoidy;
		};
		#regex {$duch[$czytroll][desc]} {ork} {
			#echo {<fca> * * * Wykrylem goblinoidy *** <088>};
			expowisko goblinoidy;
		};
		#regex {$duch[$czytroll][desc]} {goblin} {
			#echo {<fca> * * * Wykrylem goblinoidy *** <088>};
			expowisko goblinoidy;
		};
	};
}
#alias {expowisko} {
	#sh {${c_arkatt}> Ustawiam expowisko na: <138>%1};
	#line sub variables {#alias {_expo_} {%1}};
};

#alias {expowiska_lista} {
	#line ignore #sh pustkowia_chaosu;
	#line ignore #sh high_pass;
	#line ignore #sh snotlingi_nuln;
	#line ignore #sh goblinoidy;
	#line ignore #sh orki_orcal;
	#line ignore #sh orki_wsiowe;
	#line ignore #sh ryby_skellige;
	#line ignore #sh mekan;
	#line ignore #sh haszuty;
};

#alias {pustkowia_chaosu} {
	zabij trolla;
	zabij zwierzoczleka;
	_zabij_ przeciwnika;
};

#alias {high_pass} {
	zabij rycerza;
	zabij oficera;
	zabij zolnierza;
	_zabij_ przeciwnika;
};

#alias {smoczy_ogr} {
	zabij ogra;
	zabij orka;
	_zabij_ przeciwnika;
}

#alias {snotlingi_nuln} {
	zabij snotlinga;
	zabij drugiego snotlinga;
	_zabij_ przeciwnika;
};
#alias {orki_orcal} {goblinoidy};

#alias {goblinoidy} {
	zabij goblina;
	zabij drugiego orka;
	zabij orka;
	zabij czarnego orka;
	zabij hobgoblina;
	zabij trolla;
	_zabij_ przeciwnika;
};

